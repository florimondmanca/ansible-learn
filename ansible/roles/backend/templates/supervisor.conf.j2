[program:{{ backend_prog_name }}]
directory={{ workdir }}
command={{ workdir }}/venv/bin/gunicorn -w {{ backend_workers }} --bind unix:{{ backend_unix_socket }} -k uvicorn.workers.UvicornWorker {{ backend_app }}
autostart=true
autorestart=true
stderr_logfile=/var/log/{{ backend_prog_name }}.err.log
stdout_logfile=/var/log/{{ backend_prog_name }}.out.log
